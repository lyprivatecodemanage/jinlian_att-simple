<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <classPathEntry
        location="C:/java/postgresql-9.1-901.jdbc4.jar" />
    <context id="testTables">
            <!--去除注释 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>            
        <jdbcConnection 
        	driverClass="org.postgresql.Driver"
            connectionURL="jdbc:postgresql://192.168.0.242:5432/xiangshangban2_0" 
            userId="att_simple"
            password="jinnian-123abc" />            
        <!-- 生成模型的包名和位置--> 
        <javaModelGenerator targetPackage="com.xiangshangban.att_simple.bean"
            targetProject="att-simple" />
        <!-- 生成映射文件的包名和位置--> 
        <sqlMapGenerator targetPackage="com.xiangshangban.att_simple.mapper"
            targetProject="att-simple" />
        <!-- 生成DAO的包名和位置-->
        <javaClientGenerator
            targetPackage="com.xiangshangban.att_simple.dao"
            targetProject="att-simple"
            type="XMLMAPPER" />
        <!-- 要生成哪些表-->  
        <table tableName="checkingin" domainObjectName="checkingIn" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
        
    </context>
</generatorConfiguration>